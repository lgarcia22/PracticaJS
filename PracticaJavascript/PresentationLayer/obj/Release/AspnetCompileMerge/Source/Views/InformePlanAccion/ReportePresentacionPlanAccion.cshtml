@{ 
    int codigoInforme = ViewBag.codigoInforme;
    string urlMenuEncrip = ViewBag.urlMenuEncrip;
    string codInfEncrip = ViewBag.codInfEncrip;
}

<nav aria-label="breadcrumb" class="menu">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="@Url.Action("DetallePresentacionPlanAccion","InformePlanAccion" )?codInf=@codInfEncrip&urlMenu=@urlMenuEncrip">Detalle Presentación Plan Acción</a></li>
        <li class="breadcrumb-item active" aria-current="page">Reporte Presentación Plan de Acción</li>
    </ol>
</nav>
@{
    var viewerRender = Html.DevExpress().WebDocumentViewer(settings =>
    {
        settings.Name = "WebDocumentViewer";
    }).Bind(Model);
    @viewerRender.GetHtml()
}